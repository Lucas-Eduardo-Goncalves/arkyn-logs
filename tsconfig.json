{
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["./node_modules", "dist", "build"],
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "target": "ES2022",
    "moduleResolution": "Bundler",
    "jsxImportSource": "hono/jsx",
    "noEmit": true
  }
}
